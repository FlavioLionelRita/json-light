{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
      {
				"name": "order lab",
				"type": "node",
				"request": "launch",
				"skipFiles": [
					"<node_internals>/**"
				],
				"program": "${workspaceFolder}/src/dev/labs/order.ts"
			},
			{
				"name": "country lab",
				"type": "node",
				"request": "launch",
				"skipFiles": [
					"<node_internals>/**"
				],
				"program": "${workspaceFolder}/src/dev/labs/country.ts"
			},
			{
				"name": "cli help",
				"type": "node",
				"request": "launch",
				"skipFiles": [
					"<node_internals>/**"
				],
				"program": "${workspaceFolder}/src/lib/cli/json-light.ts",
				"args": [
					"help"					
				]
			},
			{
				"name": "cli schema",
				"type": "node",
				"request": "launch",
				"skipFiles": [
					"<node_internals>/**"
				],
				"program": "${workspaceFolder}/src/lib/cli/json-light.ts",
				"args": [
					"schema",
					"-i",
					"src/dev/sources/customers.json"			
				]
			},
			{
				"name": "cli compress",
				"type": "node",
				"request": "launch",
				"skipFiles": [
					"<node_internals>/**"
				],
				"program": "${workspaceFolder}/src/lib/cli/json-light.ts",
				"args": [
					"compress",
					"-i",
					"src/dev/sources/customers.json",
					"-o",
					"src/dev/results/customers.json"			
				]
			},
			{
				"name": "cli decompress",
				"type": "node",
				"request": "launch",
				"skipFiles": [
					"<node_internals>/**"
				],
				"program": "${workspaceFolder}/src/lib/cli/json-light.ts",
				"args": [
					"decompress",
					"-i",
					"src/dev/results/customers.json",	
					"-o",
					"src/dev/results/customers_original.json"			
				]
			}
    ]
}